@model IEnumerable<YontafaeOBih.WebUI.Models.ContentsViewModel>

@foreach (var gr in Model)
{
    <h3>@gr.ContentTitle</h3>
    <table class="table table-bordered table-hover">
        <tr>
            <th>
                اسم المشترك
            </th>
            <th>
                نص الرسالة
            </th>
            <th>
                تاريخ الإرسال
            </th>

        </tr>
        @foreach (var item in gr.Items)
        {
            <tr>
                <td>
                    @item.user.UserName
                </td>
                <td>
                    @item.Message
                </td>
                <td>
                    @item.ApplyDate
                </td>
            </tr>
 

        }
    </table>

}